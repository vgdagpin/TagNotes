{
  "compilerOptions": {
    "target": "ES2022",
  "module": "Node16",
    "moduleResolution": "node16",
    "outDir": "dist-electron",
    "rootDir": "electron",
    "allowJs": false,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["electron"],
  "exclude": ["dist", "dist-electron"]
}
